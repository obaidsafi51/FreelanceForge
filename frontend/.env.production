# FreelanceForge Production Environment Configuration (Paseo Testnet)

# Blockchain Network Configuration
VITE_WS_PROVIDER=wss://paseo.dotters.network
REACT_APP_WS_PROVIDER=wss://paseo.dotters.network
VITE_NETWORK=paseo
REACT_APP_NETWORK=paseo

# Paseo Testnet RPC endpoints for fallback
VITE_PASEO_RPC_1=wss://paseo.dotters.network
REACT_APP_PASEO_RPC_1=wss://paseo.dotters.network
VITE_PASEO_RPC_2=wss://rpc.ibp.network/paseo
REACT_APP_PASEO_RPC_2=wss://rpc.ibp.network/paseo
VITE_PASEO_RPC_3=wss://paseo.rpc.amforc.com
REACT_APP_PASEO_RPC_3=wss://paseo.rpc.amforc.com

# Explorer URLs
VITE_EXPLORER_URL=https://paseo.subscan.io
REACT_APP_EXPLORER_URL=https://paseo.subscan.io
VITE_PASEO_EXPLORER=https://paseo.subscan.io
REACT_APP_PASEO_EXPLORER=https://paseo.subscan.io

# Application Configuration
VITE_APP_NAME=FreelanceForge
REACT_APP_APP_NAME=FreelanceForge
VITE_VERSION=1.0.0
REACT_APP_VERSION=1.0.0

# Production Configuration
VITE_DEBUG=false
REACT_APP_DEBUG=false
VITE_LOG_LEVEL=error
REACT_APP_LOG_LEVEL=error

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
REACT_APP_ENABLE_MOCK_DATA=true
VITE_ENABLE_EXPORT=true
REACT_APP_ENABLE_EXPORT=true
VITE_ENABLE_SHARING=true
REACT_APP_ENABLE_SHARING=true

# Performance Configuration
VITE_CACHE_STALE_TIME=60000
REACT_APP_CACHE_STALE_TIME=60000
VITE_CACHE_TIME=300000
REACT_APP_CACHE_TIME=300000

# Security Configuration
VITE_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ANALYTICS=false